;-------------------------------------------------
;口上テンプレート
;獅鷹コルレオニス口上_****_**_**（キャラクターの名前を入れる、下にコンセプトなど一言あれば入れる）
;製作者　HOOVER
;-------------------------------------------------
;「条件で性格が変わる弱気な剣道少女」と宣言したはずなのに古風キャラになり、プレイがレイプ気味になり、屈服刻印で口上が変わるようになり……大人はウソつきで間違いをする生き物なのです
;-------------------------------------------------

;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K28, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 拙_せつ
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF TALENT:相思相愛
			LOCALS = 愛し君_いとしきみ
		ELSE
			LOCALS = 貴殿_きでん
		ENDIF
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:PLAYER:肩書
			CASE "実母"
				LOCALS = 母君_ははきみ
			CASE "実妹"
				;相手がさくらであなたと親密
				LOCALS = 妹君_いもうときみ
			CASEELSE
				SELECTCASE CSTR:PLAYER:学年
					CASE "初等部", "中等部"
						LOCALS = %CALLNAME:PLAYER%殿_%CALLNAME:PLAYER%どの
					CASE "１年生"
						LOCALS = 貴公_きこう
					CASE "２年生","３年生", "研究生"
						LOCALS = %CALLNAME:PLAYER%先達_%CALLNAME:PLAYER%せんたつ
				CASEELSE
					IF COND("教員", PLAYER)
						LOCALS = %CALLNAME:PLAYER%先生_%CALLNAME:PLAYER%せんせい
					ELSE
						LOCALS = 貴殿_きでん
					ENDIF
				ENDSELECT 
		ENDSELECT 
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	IF TALENT:相思相愛
		LOCALS = 我が君_わがきみ
	ELSE
		LOCALS = 部長殿_ぶちょうどの
	ENDIF
CASE "助手"
	LOCALS = %CALLNAME:ASSI%殿_%CALLNAME:ASSI%どの
ENDSELECT
RETURNF LOCALS

